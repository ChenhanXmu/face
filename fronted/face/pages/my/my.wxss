.table {
  margin-top: 60rpx;
  border: 0px solid darkgray;
}
.tr {
  display: flex;
  width: 600rpx;
  justify-content: center;
  height: 3rem;
  align-items: center;
}
.td {
    width:40%;
    justify-content: center;
    text-align: center;
}
.bg-w{
  background: snow;
}
.bg-g{
  background: #E6F3F9;
}
.th {
  width: 40%;
